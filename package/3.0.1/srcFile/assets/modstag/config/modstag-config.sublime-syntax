        - match: ','
          scope: minecraft.modstag.color.tips
          set:
            - match: '\)'
              scope: minecraft.modstag.color.error-key
            - include: config-file
        - include: config.end

  config-file-base2: 
    - match: '[0-1]{32}|[0-1]{16}|[0-1]{8}'
      scope: minecraft.modstag.color.green
      set: 
        - match: '\n'
          set: config.end
        - match: '\s,|,'
          scope: minecraft.modstag.color.tips
          set: config-file
        - match: '\s'
          set: config-file-base2
        - include: config.end
    - match: '[^\s\n]'
      scope: minecraft.modstag.color.error-key
