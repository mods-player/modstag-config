              scope: minecraft.modstag.color.aqua
              set: java-config-file-listExtra-dict-list-dict-json-extra-clickEvent-value
  java-config-file-listExtra-dict-list-dict-json-extra-clickEvent.end:
    - match: '\}'
      scope: minecraft.modstag.color.gold
      set:
        - match: ',[\s\n]'
          scope: minecraft.modstag.color.tips
          set: 
            - match: '\}'
              scope: minecraft.modstag.color.error-key
            - include: java-config-file-listExtra-dict-list-dict-json-extra
        - include: java-config-file-listExtra-dict-list-dict-json-extra.end
  java-config-file-listExtra-dict-list-dict-json-extra-clickEvent-action:
    - match: '\"'
      scope: minecraft.modstag.color.aqua
      set:
        - match: ',[\s\n]'
          scope: minecraft.modstag.color.tips
          set: java-config-file-listExtra-dict-list-dict-json-extra-clickEvent
        - include: java-config-file-listExtra-dict-list-dict-json-extra-clickEvent.end
    - match: '\n'
      scope: minecraft.modstag.color.error-key
      set:
        - clear_scopes: 1
    - match: 'run_(command|url|herf|code)'
      scope: minecraft.modstag.color.yellow 
  java-config-file-listExtra-dict-list-dict-json-extra-clickEvent-value:
    - match: '\"'
      scope: minecraft.modstag.color.aqua
      set:
        - match: ',[\s\n]'
          scope: minecraft.modstag.color.tips
          set: java-config-file-listExtra-dict-list-dict-json-extra-clickEvent
        - include: java-config-file-listExtra-dict-list-dict-json-extra-clickEvent.end
    - match: '\n'
      scope: minecraft.modstag.color.error-key
      set:
        - clear_scopes: 1
    - match: '\\[bfnrt\"\\]|\\u[0-9a-fA-F]{4}'
      scope: minecraft.modstag.color.purple
    - match: '\\'
      scope: minecraft.modstag.color.error-key
    - match: '[^\s\n\"]'
      scope: minecraft.modstag.color.yellow
