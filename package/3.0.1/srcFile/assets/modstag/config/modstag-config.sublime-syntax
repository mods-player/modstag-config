%YAML 1.2
---
name: modstag config
scope: source.modstag-cfg

file_extensions:
  - modstag-cfg
  - modstag-config

first_line_match: '@modstag-config|@modstag-cfg'

contexts:
  prototype:
    - match: '//tips:'
      scope: minecraft.modstag.color.tips
      push:
        - meta_scope: minecraft.modstag.color.tips
        - meta_include_prototype: false
        - match: ':end//'
          scope: minecraft.modstag.color.tips
          pop: 1
    - match: '#tips:.*$\n?'
      scope: minecraft.modstag.color.tips
  main:
    - include: firstmatch
    - include: package
    - include: readme
    - include: run
  firstmatch:
    - match: '!tips: .*$\n?'
      scope: minecraft.modstag.color.tips
    - match: '(@modstag-config|@modstag-cfg)\('
      scope: minecraft.modstag.color.italic_red
      set: 
        - include: firstmatch-type
        - include: firstmatch-type.end
  firstmatch-type:
    - match: '(version)(:)'
      captures:
        1: minecraft.modstag.color.italic_red
        2: minecraft.modstag.color.italic_tips
      set:
        - include: firstmatch-type-version
    - match: '(maincode)(:)'
      captures:
        1: minecraft.modstag.color.italic_red
        2: minecraft.modstag.color.italic_tips
      set:
        - include: firstmatch-type-maincode
  firstmatch-type.end:
    - match: '\)'
      scope: minecraft.modstag.color.italic_red
      set:
        - include: package
        - include: readme
        - include: run
  firstmatch-type-version:
    - match: '3\.[0-9]\.[0-9][0-9]|3\.[0-9]\.[0-9]'
      scope: minecraft.modstag.color.italic_red
      set:
        - match: ','
          scope: minecraft.modstag.color.italic_tips
          set:
            - include: firstmatch-type
        - include: firstmatch-type.end
    - match: '3\.[0-9]\.[0-9][0-9][+-]|3\.[0-9]\.[0-9][+-]'
      scope: minecraft.modstag.color.italic_red
      set:
        - match: ','
          scope: minecraft.modstag.color.italic_tips
          set:
            - include: firstmatch-type
        - include: firstmatch-type.end
  firstmatch-type-maincode:
    - match: 'modstag|python3|python|java|cs|cpp|c++|c'
      scope: minecraft.modstag.color.italic_red
      set:
        - match: ','
          scope: minecraft.modstag.color.italic_tips
          set:
            - include: firstmatch-type
        - include: firstmatch-type.end
  package:
    - match: '!tips: .*$\n?'
      scope: minecraft.modstag.color.tips
    - match: '(package)(\s:\s|:\s)'
      captures: 
        1: minecraft.modstag.color.dark_red
        2: minecraft.modstag.color.tips
      set:
        - meta_scope: minecraft.modstag.color.dark_aqua
        - meta_include_prototype: false
        - match: '\\\\'
          scope: minecraft.modstag.color.dark_aqua
        - include: run
        - match: '[\\/:\?\*<>\|\"]'
          scope: minecraft.modstag.color.error-key
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - include: run
            - include: readme
  readme:
    - match: '!tips: .*$\n?'
      scope: minecraft.modstag.color.tips
    - match: '(readme|README|Readme)(\s:\s|:\s)'
      captures:
        1: minecraft.modstag.color.dark_red
        2: minecraft.modstag.color.tips
      set:
        - meta_scope: minecraft.modstag.color.dark_aqua
        - meta_include_prototype: false
        - match: '\\\\'
          scope: minecraft.modstag.color.dark_aqua
        - include: run
        - match: '[\\/:\?\*<>\|\"]'
          scope: minecraft.modstag.color.error-key
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - include: run
            - include: package
        - include: run
  run:
    - match: '!tips: .*$\n?'
      scope: minecraft.modstag.color.tips
    - match: 'run\('
      scope: minecraft.modstag.color.dark_purple
      set:
        - match: 'modstag'
          scope: minecraft.modstag.color.dark_aqua
          set:
            - match: '\)\n'
              scope: minecraft.modstag.color.dark_purple
              set: modstag-config
        - match: 'cs'
          scope: minecraft.modstag.color.dark_aqua
          set:
            - match: '\)\n'
              scope: minecraft.modstag.color.dark_purple
              set: cs-config
        - match: 'cpp|c\+\+'
          scope: minecraft.modstag.color.dark_aqua
          set:
            - match: '\)\n'
              scope: minecraft.modstag.color.dark_purple
              set: cpp-config
        - match: 'c'
          scope: minecraft.modstag.color.dark_aqua
          set:
            - match: '\)\n'
              scope: minecraft.modstag.color.dark_purple
              set: c-config
        - match: '\)\n'
          scope: minecraft.modstag.color.dark_purple
          set: config
            
  config:
    - match: '%'
      scope: minecraft.modstag.color.red
      set:
        - match: '%|&'
          scope: minecraft.modstag.color.error-key
        - match: '(cfg)(\.)'
          captures: 
            1: minecraft.modstag.color.aqua
            2: minecraft.modstag.color.tips
          set:
            - match: '(file|file(\[)(string|text|byte|short|int|long|float|double|decimal|boolean|void)(\]))(\.)'
              captures: 
                1: minecraft.modstag.color.aqua
                2: minecraft.modstag.color.red
                3: minecraft.modstag.color.aqua
                4: minecraft.modstag.color.red
                5: minecraft.modstag.color.tips
              set:
                - match: '\\[%&;\\\[\]:]'
                  scope: minecraft.modstag.color.purple
                - match: '[\[\]:]'
                  scope: minecraft.modstag.color.red
                - match: ';'
                  scope: minecraft.modstag.color.tips
                - match: '&|\\'
                  scope: minecraft.modstag.color.error-key
                - match: '%'
                  scope: minecraft.modstag.color.red
                  set:
                    - match: ':'
                      scope: minecraft.modstag.color.tips
                      set:
                        - match: '\('
                          scope: minecraft.modstag.color.gold
                          set:
                            - include: config.end
                            - include: config-file
                        - match: '\n'
                          scope: minecraft.modstag.color.textwhite
                          push:
                            - clear_scopes: 1
                    - match: '\n'
                      scope: minecraft.modstag.color.textwhite
                      push:
                        - clear_scopes: 1
                - match: '\n'
                  scope: minecraft.modstag.color.textwhite
                  push:
                    - clear_scopes: 1
                - match: '[^\s\n]'
                  scope: minecraft.modstag.color.aqua
            - match: '\n'
              scope: minecraft.modstag.color.textwhite
              push:
                - clear_scopes: 1
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
    - match: '&'
      scope: minecraft.modstag.color.red
      set:
        - match: '\\[%&\\\.]'
          scope: minecraft.modstag.color.purple
        - match: '\.|%'
          scope: minecraft.modstag.color.error-key
        - match: '&'
          scope: minecraft.modstag.color.red
          push:
            - match: ':'
              scope: minecraft.modstag.color.tips
              set:
                - match: '\('
                  scope: minecraft.modstag.color.gold
                  set:
                    - include: config.end
                    - include: config-file
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
        - match: '[^\&]'
          scope: minecraft.modstag.color.aqua
    - match: '[^\s\n\:]'
      scope: minecraft.modstag.color.error-key
  config.end:
    - match: '\)'
      scope: minecraft.modstag.color.gold
      set:
        - match: ';'
          scope: minecraft.modstag.color.tips
          set:
            - include: run
  config-file:
    - match: '%'
      scope: minecraft.modstag.color.red
      set:
        - match: '%|&'
          scope: minecraft.modstag.color.error-key
        - match: '(cfg)(\.)'
          captures: 
            1: minecraft.modstag.color.aqua
            2: minecraft.modstag.color.tips
          set:
            - match: '(string)(\.)'
              captures: 
                1: minecraft.modstag.color.aqua
                2: minecraft.modstag.color.tips
              set:
                - match: '\\[%&;\\\[\]:]'
                  scope: minecraft.modstag.color.purple
                - match: '[\[\]:]'
                  scope: minecraft.modstag.color.red
                - match: ';'
                  scope: minecraft.modstag.color.gray
                - match: '&|\\'
                  scope: minecraft.modstag.color.error-key
                - match: '%'
                  scope: minecraft.modstag.color.red
                  set:
                    - match: '\n'
                      scope: minecraft.modstag.color.error-key
                      set:
                        - clear_scopes: 1
                        - include: config-file
                    - match: ':'
                      scope: minecraft.modstag.color.tips
                      set: 
                        - match: '\n'
                          scope: minecraft.modstag.color.error-key
                          set:
                            - clear_scopes: 1
                            - include: config-file
                        - include: config-file-string
                - match: '\n'
                  scope: minecraft.modstag.color.error-key
                  set:
                    - clear_scopes: 1
                    - include: config-file
                - match: '[^\s\n]'
                  scope: minecraft.modstag.color.aqua
            - match: '(text)(\.)'
              captures: 
                1: minecraft.modstag.color.aqua
                2: minecraft.modstag.color.tips
              set:
                - match: '\\[%&;\\\[\]:]'
                  scope: minecraft.modstag.color.purple
                - match: '[\[\]:]'
                  scope: minecraft.modstag.color.red
                - match: ';'
                  scope: minecraft.modstag.color.gray
                - match: '&|\\'
                  scope: minecraft.modstag.color.error-key
                - match: '%'
                  scope: minecraft.modstag.color.red
                  set:
                    - match: '\n'
                      scope: minecraft.modstag.color.error-key
                      set:
                        - clear_scopes: 1
                        - include: config-file
                    - match: ':'
                      scope: minecraft.modstag.color.tips
                      set: 
                        - match: '\n'
                          scope: minecraft.modstag.color.error-key
                          set:
                            - clear_scopes: 1
                            - include: config-file
                        - include: config-file-text
                - match: '\n'
                  scope: minecraft.modstag.color.error-key
                  set:
                    - clear_scopes: 1
                    - include: config-file
                - match: '[^\s\n]'
                  scope: minecraft.modstag.color.aqua
            - match: '(byte)(\.)'
              captures: 
                1: minecraft.modstag.color.aqua
                2: minecraft.modstag.color.tips
              set:
                - match: '\\[%&;\\\[\]:]'
