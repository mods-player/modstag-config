  cpp-config-file-boolean:
    - match: 'false|true|null'
      scope: minecraft.modstag.color.italic_red
      set:
        - match: ','
          scope: minecraft.modstag.color.tips
          set:
            - match: '\)'
              scope: minecraft.modstag.color.error-key
            - include: cpp-config-file
        - include: cpp-config.end

  cpp-config-file-math:
    - match: '\.'
      scope: minecraft.modstag.color.tips
      set:
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - clear_scopes: 1
        - match: '\=\='
          scope: minecraft.modstag.color.error-key
        - match: '\.\.'
          scope: minecraft.modstag.color.green
        - match: '>>|<<'
          scope: minecraft.modstag.color.red
        - match: '[\+\-\*\^\/\(\)]'
          scope: minecraft.modstag.color.red
        - match: '[0-9]'
          scope: minecraft.modstag.color.green
        - include: cpp-config-file-math.end
        - match: '[^\.\<\>]'
          scope: minecraft.modstag.color.error-key
    - match: '\n'
      scope: minecraft.modstag.color.textwhite
      set:
        - clear_scopes: 1
    - match: '\=\='
      scope: minecraft.modstag.color.error-key
    - match: '\.\.'
      scope: minecraft.modstag.color.green
    - match: '>>|<<'
      scope: minecraft.modstag.color.red
    - match: '[\+\-\*\^\/\(\)]'
      scope: minecraft.modstag.color.red
    - match: '[0-9]'
      scope: minecraft.modstag.color.green
    - match: '%s|''%s'''
      scope: minecraft.modstag.color.yellow
      set:
        - match: '\n'
          scope: minecraft.modstag.color.error-key
          set:
            - clear_scopes: 1
        - include: cpp-config-file-math.end
        - match: '\=\='
          scope: minecraft.modstag.color.purple
          set: cpp-config-file-math-%s
    - include: cpp-config-file-math.end
    - match: '[^\.\<\>]'
      scope: minecraft.modstag.color.error-key
  cpp-config-file-math.end:
    - match: '>'
      scope: minecraft.modstag.color.gold
      set:
        - match: ','
          scope: minecraft.modstag.color.tips
          set:
            - match: '\)'
              scope: minecraft.modstag.color.error-key
            - include: cpp-config-file
        - include: cpp-config.end
  cpp-config-file-math-%s:
    - match: '\.'
      scope: minecraft.modstag.color.tips
      set:
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - clear_scopes: 1
        - match: '([\+\-\*\/])(%s|''%s'')'
          captures:
            1: minecraft.modstag.color.red
            2: minecraft.modstag.color.yellow
        - match: '\=\='
          scope: minecraft.modstag.color.error-key
        - match: '\.\.'
          scope: minecraft.modstag.color.green
        - match: '>>|<<'
          scope: minecraft.modstag.color.red
        - match: '[\+\-\~\*\^\/\(\)]'
          scope: minecraft.modstag.color.red
        - match: '[0-9]'
          scope: minecraft.modstag.color.green
        - include: cpp-config-file-math.end
        - match: '[^\.\<\>]'
          scope: minecraft.modstag.color.error-key
    - match: '\n'
      scope: minecraft.modstag.color.error-key
      set:
        - clear_scopes: 1
    - match: '([\+\-\*\/])(%s|''%s'')'
      captures:
        1: minecraft.modstag.color.red
        2: minecraft.modstag.color.yellow
    - match: '\=\='
      scope: minecraft.modstag.color.error-key
    - match: '\.\.'
      scope: minecraft.modstag.color.green
    - match: '>>|<<'
      scope: minecraft.modstag.color.red
    - match: '[\+\-\~\*\^\/\(\)]'
      scope: minecraft.modstag.color.red
    - match: '[0-9]'
      scope: minecraft.modstag.color.green
    - include: cpp-config-file-math.end
    - match: '[^\.\<\>]'
      scope: minecraft.modstag.color.error-key
  cpp-config-file-base2: 
    - match: '[0-1]{32}|[0-1]{16}|[0-1]{8}'
      scope: minecraft.modstag.color.green
      set: 
        - match: '\n'
          set: cpp-config.end
        - match: '\s,|,'
