  c-config-file-boolean:
    - match: 'false|true|null'
      scope: minecraft.modstag.color.italic_red
      set:
        - match: ','
          scope: minecraft.modstag.color.tips
          set:
            - match: '\)'
              scope: minecraft.modstag.color.error-key
            - include: c-config-file
        - include: c-config.end

  c-config-file-math:
    - match: '\.'
      scope: minecraft.modstag.color.tips
      set:
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - clear_scopes: 1
        - match: '\=\='
          scope: minecraft.modstag.color.error-key
        - match: '\.\.'
          scope: minecraft.modstag.color.green
        - match: '>>|<<'
          scope: minecraft.modstag.color.red
        - match: '[\+\-\*\^\/\(\)]'
          scope: minecraft.modstag.color.red
        - match: '[0-9]'
          scope: minecraft.modstag.color.green
        - include: c-config-file-math.end
        - match: '[^\.\<\>]'
          scope: minecraft.modstag.color.error-key
    - match: '\n'
      scope: minecraft.modstag.color.textwhite
      set:
        - clear_scopes: 1
    - match: '\=\='
      scope: minecraft.modstag.color.error-key
    - match: '\.\.'
      scope: minecraft.modstag.color.green
    - match: '>>|<<'
      scope: minecraft.modstag.color.red
    - match: '[\+\-\*\^\/\(\)]'
      scope: minecraft.modstag.color.red
    - match: '[0-9]'
      scope: minecraft.modstag.color.green
    - match: '%s|''%s'''
      scope: minecraft.modstag.color.yellow
      set:
        - match: '\n'
          scope: minecraft.modstag.color.error-key
          set:
            - clear_scopes: 1
        - include: c-config-file-math.end
        - match: '\=\='
          scope: minecraft.modstag.color.purple
          set: c-config-file-math-%s
    - include: c-config-file-math.end
    - match: '[^\.\<\>]'
      scope: minecraft.modstag.color.error-key
  c-config-file-math.end:
    - match: '>'
      scope: minecraft.modstag.color.gold
      set:
        - match: ','
          scope: minecraft.modstag.color.tips
          set:
            - match: '\)'
              scope: minecraft.modstag.color.error-key
            - include: c-config-file
        - include: c-config.end
  c-config-file-math-%s:
    - match: '\.'
      scope: minecraft.modstag.color.tips
      set:
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - clear_scopes: 1
        - match: '([\+\-\*\/])(%s|''%s'')'
          captures:
            1: minecraft.modstag.color.red
            2: minecraft.modstag.color.yellow
        - match: '\=\='
          scope: minecraft.modstag.color.error-key
        - match: '\.\.'
          scope: minecraft.modstag.color.green
        - match: '>>|<<'
          scope: minecraft.modstag.color.red
        - match: '[\+\-\~\*\^\/\(\)]'
          scope: minecraft.modstag.color.red
        - match: '[0-9]'
          scope: minecraft.modstag.color.green
        - include: c-config-file-math.end
        - match: '[^\.\<\>]'
          scope: minecraft.modstag.color.error-key
    - match: '\n'
      scope: minecraft.modstag.color.error-key
      set:
        - clear_scopes: 1
    - match: '([\+\-\*\/])(%s|''%s'')'
      captures:
        1: minecraft.modstag.color.red
        2: minecraft.modstag.color.yellow
    - match: '\=\='
      scope: minecraft.modstag.color.error-key
    - match: '\.\.'
      scope: minecraft.modstag.color.green
    - match: '>>|<<'
      scope: minecraft.modstag.color.red
    - match: '[\+\-\~\*\^\/\(\)]'
      scope: minecraft.modstag.color.red
    - match: '[0-9]'
      scope: minecraft.modstag.color.green
    - include: c-config-file-math.end
    - match: '[^\.\<\>]'
      scope: minecraft.modstag.color.error-key
  c-config-file-base2: 
    - match: '[0-1]{32}|[0-1]{16}|[0-1]{8}'
      scope: minecraft.modstag.color.green
      set: 
        - match: '\n'
          set: c-config.end
        - match: '\s,|,'
          scope: minecraft.modstag.color.tips
          set: c-config-file
        - match: '\s'
          set: c-config-file-base2
        - include: c-config.end
    - match: '[^\s\n]'
      scope: minecraft.modstag.color.error-key
  c-config-file-base16: 
    - match: '[0-9a-fA-F]{8}|[0-9a-fA-F]{4}|[0-9a-fA-F]{2}'
      scope: minecraft.modstag.color.green
      set: 
        - match: '\n'
          set: c-config.end
        - match: '\s,|,'
          scope: minecraft.modstag.color.tips
          set: c-config-file
        - match: '\s'
          set: c-config-file-base16
        - include: c-config.end
    - match: '[^\s\n]'
      scope: minecraft.modstag.color.error-key

  c-config-file-listString:
    - match: '\"'
