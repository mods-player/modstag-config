  modstag-config-file-listExtra-dict-list-dict-json-extra.end:
    - match: '\}'
      scope: minecraft.modstag.color.gold
      set:
        - match: ',[\s\n]'
          scope: minecraft.modstag.color.tips
          set: 
            - match: '\}'
              scope: minecraft.modstag.color.error-key
            - include: modstag-config-file-listExtra-dict-list-dict-json
        - include: modstag-config-file-listExtra-dict-list-dict-json.end
