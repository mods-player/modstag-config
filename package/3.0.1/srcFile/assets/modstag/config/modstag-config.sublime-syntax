      scope: minecraft.modstag.color.error-key

  cpp-config-file-std_string:
    - match: '\"'
      scope: minecraft.modstag.color.aqua
      set:
        - match: '\"'
          scope: minecraft.modstag.color.aqua
          set:
            - match: ','
              scope: minecraft.modstag.color.tips
              set:
                - match: '\)'
                  scope: minecraft.modstag.color.error-key
                - include: cpp-config-file
            - include: cpp-config.end
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - clear_scopes: 1
        - match: '\\[bfnrt\"\\]|\\u[0-9a-fA-F]{4}'
          scope: minecraft.modstag.color.purple
        - match: '\\'
          scope: minecraft.modstag.color.error-key
        - match: '[^\s\n\"]'
          scope: minecraft.modstag.color.yellow
  cpp-config-file-int8:
    - match: '\"\"|'''''
      scope: minecraft.modstag.color.aqua
      set:
        - match: ','
          scope: minecraft.modstag.color.tips
          set:
            - match: '\)'
              scope: minecraft.modstag.color.error-key
            - include: cpp-config-file
        - include: cpp-config.end
    - match: '(\")([^\n\"])(\")|('')([^\n\"])('')'
      captures:
        1: minecraft.modstag.color.aqua
        2: minecraft.modstag.color.yellow
        3: minecraft.modstag.color.aqua
      set:
        - match: ','
          scope: minecraft.modstag.color.tips
          set:
            - match: '\)'
              scope: minecraft.modstag.color.error-key
            - include: cpp-config-file
        - include: cpp-config.end
