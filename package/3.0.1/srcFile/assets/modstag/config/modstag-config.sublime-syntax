  cs-config:
    - match: '%'
      scope: minecraft.modstag.color.red
      set:
        - match: '%|&'
          scope: minecraft.modstag.color.error-key
        - match: '(cfg)(\.)'
          captures: 
            1: minecraft.modstag.color.aqua
            2: minecraft.modstag.color.tips
          set:
            - match: '(file|file(\[)(sbyte|byte|short|int|long|ushort|uint|ulong|float|double|decimal|boolean|void)(\]))(\.)'
              captures: 
                1: minecraft.modstag.color.aqua
                2: minecraft.modstag.color.red
                3: minecraft.modstag.color.aqua
                4: minecraft.modstag.color.red
                5: minecraft.modstag.color.tips
              set:
                - match: '\\[%&;\\\[\]:]'
                  scope: minecraft.modstag.color.purple
                - match: '[\[\]:]'
                  scope: minecraft.modstag.color.red
                - match: ';'
                  scope: minecraft.modstag.color.tips
                - match: '&|\\'
                  scope: minecraft.modstag.color.error-key
                - match: '%'
                  scope: minecraft.modstag.color.red
                  set:
                    - match: ':'
                      scope: minecraft.modstag.color.tips
                      set:
                        - match: '\('
                          scope: minecraft.modstag.color.gold
                          set:
                            - include: cs-config.end
                            - include: cs-config-file
                        - match: '\n'
                          scope: minecraft.modstag.color.textwhite
                          push:
                            - clear_scopes: 1
                    - match: '\n'
                      scope: minecraft.modstag.color.textwhite
                      push:
                        - clear_scopes: 1
                - match: '\n'
                  scope: minecraft.modstag.color.textwhite
                  push:
                    - clear_scopes: 1
                - match: '[^\s\n]'
                  scope: minecraft.modstag.color.aqua
            - match: '\n'
              scope: minecraft.modstag.color.textwhite
              push:
                - clear_scopes: 1
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
    - match: '&'
      scope: minecraft.modstag.color.red
      set:
        - match: '\\[%&\\\.]'
          scope: minecraft.modstag.color.purple
        - match: '\.|%'
          scope: minecraft.modstag.color.error-key
        - match: '&'
          scope: minecraft.modstag.color.red
          push:
            - match: ':'
              scope: minecraft.modstag.color.tips
              set:
                - match: '\('
                  scope: minecraft.modstag.color.gold
                  set:
                    - include: cs-config.end
                    - include: cs-config-file
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
        - match: '[^\&]'
          scope: minecraft.modstag.color.aqua
    - match: '[^\s\n\:]'
      scope: minecraft.modstag.color.error-key
  cs-config.end:
    - match: '\)'
      scope: minecraft.modstag.color.gold
      set:
        - match: ';'
          scope: minecraft.modstag.color.tips
          set:
            - include: run
