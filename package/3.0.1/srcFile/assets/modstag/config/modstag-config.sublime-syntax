        - match: '\n'
          scope: minecraft.modstag.color.error-key
          set:
            - clear_scopes: 1
        - match: ':'
          scope: minecraft.modstag.color.tips
          set: 
            - match: '\"'
              scope: minecraft.modstag.color.aqua
              set: c-config-file-listExtra-dict-list-dict-json-extra-clickEvent-value
