              set: c-config-file-listExtra-dict-list-dict-json-extra-clickEvent-action
    - match: '(\")(value)(\")'
      captures: 
        1: minecraft.modstag.color.aqua
        2: minecraft.modstag.color.yellow
        3: minecraft.modstag.color.aqua
