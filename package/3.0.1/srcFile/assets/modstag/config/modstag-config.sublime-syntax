            - clear_scopes: 1
        - match: '[^\&]'
          scope: minecraft.modstag.color.aqua
    - match: '[^\s\n\:]'
      scope: minecraft.modstag.color.error-key
  modstag-config.end:
    - match: '\)'
      scope: minecraft.modstag.color.gold
      set:
        - match: ';'
          scope: minecraft.modstag.color.tips
          set:
            - include: run
  modstag-config-file:
