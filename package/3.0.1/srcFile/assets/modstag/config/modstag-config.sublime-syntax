                            - clear_scopes: 1
                            - include: config-file
                        - include: config-file-short
                - match: '\n'
                  scope: minecraft.modstag.color.error-key
                  set:
                    - clear_scopes: 1
                    - include: config-file
                - match: '[^\s\n]'
                  scope: minecraft.modstag.color.aqua
