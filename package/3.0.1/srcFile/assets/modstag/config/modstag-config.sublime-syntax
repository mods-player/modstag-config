                          scope: minecraft.modstag.color.error-key
                          set:
                            - clear_scopes: 1
                            - include: java-config-file
                        - match: '\['
                          scope: minecraft.modstag.color.gold
                          set: 
                            - include: java-config-file-listBoolean
                            - match: '\]'
                              scope: minecraft.modstag.color.gold
                              set:
                                - match: ','
                                  scope: minecraft.modstag.color.tips
                                  set:
                                    - match: '\)'
                                      scope: minecraft.modstag.color.error-key
                                    - include: java-config-file
                                - include: java-config.end
                - match: '\n'
                  scope: minecraft.modstag.color.error-key
                  set:
                    - clear_scopes: 1
                    - include: java-config-file
                - match: '[^\s\n]'
                  scope: minecraft.modstag.color.aqua
            - match: '[^\s\n\.]'
              scope: minecraft.modstag.color.error-key
            - match: '\n'
              scope: minecraft.modstag.color.textwhite
              push:
                - clear_scopes: 1
                - include: java-config-file
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
            - include: java-config-file
    - match: '&'
      scope: minecraft.modstag.color.red
      set:
        - match: '\\[%&\\\.]'
          scope: minecraft.modstag.color.purple
        - match: '\.|%'
          scope: minecraft.modstag.color.error-key
        - match: '&'
          scope: minecraft.modstag.color.red
          set:
            - match: '\n'
              scope: minecraft.modstag.color.textwhite
              push:
                - clear_scopes: 1
                - include: java-config-file
            - match: ':'
              scope: minecraft.modstag.color.tips
              set:
                - include: java-config-file-setting
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
            - include: java-config-file
        - match: '[^\&]'
          scope: minecraft.modstag.color.aqua
    - match: '[^\s\n\:]'
      scope: minecraft.modstag.color.error-key

