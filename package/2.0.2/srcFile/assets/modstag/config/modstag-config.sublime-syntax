%YAML 1.2
---
version: 2.0.2
name: modstag config
scope: source.modstag-cfg

file_extensions:
  - modstag-cfg
  - modstag-config

first_line_match: '@modstag-config|@modstag-cfg'

contexts:
  prototype:
    - match: '//tips: .*$\n?'
      scope: minecraft.modstag.color.tips
    - match: '#tips: .*$\n?'
      scope: minecraft.modstag.color.tips  
  main:
    - match: '@modstag-config|@modstag-cfg'
      scope: minecraft.modstag.color.false-true
    - match: '@modstag-version [0-9].[0-9].[0-9][+-]|@modstag-ver [0-9].[0-9].[0-9][+-]'
      scope: minecraft.modstag.color.false-true
    - match: '@modstag-version [0-9].[0-9].[0-9]|@modstag-ver [0-9].[0-9].[0-9]'
      scope: minecraft.modstag.color.false-true
    - include: package
    - include: run
  package:
    - match: '!tips: .*$\n?'
      scope: minecraft.modstag.color.tips
    - match: 'package'
      scope: minecraft.modstag.color.dark_red
      set:
        - match: ' : |: '
          scope: minecraft.modstag.color.tips
          set: 
            - match: '\\\\'
              scope: minecraft.modstag.color.dark_aqua
            - include: run
            - match: '[\\/:\?\*<>\|\"]'
              scope: minecraft.modstag.color.error-key
            - match: '[^\s\n]'
              scope: minecraft.modstag.color.dark_aqua
            - match: '\n'
              scope: minecraft.modstag.color.textwhite
              set:
                - include: run
                - include: readme
        - match: ':| :'
          scope: minecraft.modstag.color.tips
        - match: '\s\s|\s'
          scope: minecraft.modstag.color.error-key
        - include: main
  readme:
    - match: '!tips: .*$\n?'
      scope: minecraft.modstag.color.tips
    - match: 'readme|README|Readme'
      scope: minecraft.modstag.color.dark_red
      set:
        - match: ' : |: '
          scope: minecraft.modstag.color.tips
          set:
            - match: '\\\\'
              scope: minecraft.modstag.color.dark_aqua
            - include: run
            - match: '[\\/:\?\*<>\|\"]'
              scope: minecraft.modstag.color.error-key
            - match: '[^\s\n]'
              scope: minecraft.modstag.color.dark_aqua
            - match: '\n'
              scope: minecraft.modstag.color.textwhite
              set:
                - include: run
                - include: readme
            - include: run
        - match: ':| :'
          scope: minecraft.modstag.color.tips
        - match: '\s\s|\s'
          scope: minecraft.modstag.color.error-key
        - include: main
  run:
    - match: 'run\('
      scope: minecraft.modstag.color.dark_purple
      push:
        - match: '\)\n'
          scope: minecraft.modstag.color.dark_purple
          push:
            - include: config
            
  config:
    - match: '%'
      scope: minecraft.modstag.color.red
      set:
        - match: '%|&'
          scope: minecraft.modstag.color.error-key
        - match: 'cfg'
          scope: minecraft.modstag.color.aqua
          set:
            - match: '\.'
              scope: minecraft.modstag.color.tips
              set:
                - match: 'file'
                  scope: minecraft.modstag.color.aqua
                  set:
                    - match: '\.'
                      scope: minecraft.modstag.color.tips
                      set:
                        - match: '\\[%&;\\]'
                          scope: minecraft.modstag.color.purple
                        - match: '[\[\]:]'
                          scope: minecraft.modstag.color.red
                        - match: ';'
                          scope: minecraft.modstag.color.gray
                        - match: '&|\\'
                          scope: minecraft.modstag.color.error-key
                        - match: '%'
                          scope: minecraft.modstag.color.red
                          set:
                            - match: ':'
                              scope: minecraft.modstag.color.tips
                              set:
                                - match: '\('
                                  scope: minecraft.modstag.color.gold
                                  set:
                                    - include: config.end
                                    - include: config-file
                        - match: '\n'
                          scope: minecraft.modstag.color.textwhite
                          push:
                            - clear_scopes: 1
                            - include: config
                        - match: '[^\s\n]'
                          scope: minecraft.modstag.color.aqua
    - match: '&'
      scope: minecraft.modstag.color.red
      set:
        - match: '\.|%'
          scope: minecraft.modstag.color.error-key
        - match: '&'
          scope: minecraft.modstag.color.red
          push:
            - match: ':'
              scope: minecraft.modstag.color.tips
              set:
                - match: '\('
                  scope: minecraft.modstag.color.gold
                  set:
                    - include: config.end
                    - include: config-file
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
            - include: config
        - match: '[^\&]'
          scope: minecraft.modstag.color.aqua
    - match: '[^\s\n\:]'
      scope: minecraft.modstag.color.error-key
  config.end:
    - match: '\)'
      scope: minecraft.modstag.color.gold
      set:
        - match: ';'
          scope: minecraft.modstag.color.tips
          set:
            - include: config
  config-file:
    - match: '%'
      scope: minecraft.modstag.color.red
      set:
        - match: '%|&'
          scope: minecraft.modstag.color.error-key
        - match: 'cfg'
          scope: minecraft.modstag.color.aqua
          set:
            - match: '\.'
              scope: minecraft.modstag.color.tips
              set:
                - match: 'string'
                  scope: minecraft.modstag.color.aqua
                  set:
                    - match: '\.'
                      scope: minecraft.modstag.color.tips
                      set:
                        - match: '\\[%&;\\]'
                          scope: minecraft.modstag.color.purple
                        - match: '[\[\]:]'
                          scope: minecraft.modstag.color.red
                        - match: ';'
                          scope: minecraft.modstag.color.gray
                        - match: '&|\\'
                          scope: minecraft.modstag.color.error-key
                        - match: '%'
                          scope: minecraft.modstag.color.red
                          set:
                            - match: '\n'
                              scope: minecraft.modstag.color.error-key
                              set:
                                - clear_scopes: 1
                            - match: ':'
                              scope: minecraft.modstag.color.tips
                              set: 
                                - match: '\n'
                                  scope: minecraft.modstag.color.error-key
                                  set:
                                    - clear_scopes: 1
                                - include: config-file-string
                        - match: '\n'
                          scope: minecraft.modstag.color.error-key
                          set:
                            - clear_scopes: 1
                            - include: config
                        - match: '[^\s\n]'
                          scope: minecraft.modstag.color.aqua
                - match: 'text'
                  scope: minecraft.modstag.color.aqua
                  set:
