%YAML 1.2
---
name: modstag config
scope: source.modstag-cfg

file_extensions:
  - modstag-cfg
  - modstag-config

first_line_match: '@modstag-config|@modstag-cfg'

contexts:
  prototype:
    - match: '//tips:'
      scope: minecraft.modstag.color.tips
      push:
        - meta_scope: minecraft.modstag.color.tips
        - meta_include_prototype: false
        - match: ':end//'
          pop: 1
    - match: '#tips:.*$\n?'
      scope: minecraft.modstag.color.tips
  main:
    - match: '(@modstag-config|@modstag-cfg)(\(\)|\(python\))'
      scope: minecraft.modstag.color.false-true
    - match: '@modstag-version [0-9].[0-9].[0-9][+-]|@modstag-ver [0-9].[0-9].[0-9][+-]'
      scope: minecraft.modstag.color.false-true
    - match: '@modstag-version [0-9].[0-9].[0-9]|@modstag-ver [0-9].[0-9].[0-9]'
      scope: minecraft.modstag.color.false-true
    - include: package
    - include: run
  package:
    - match: '(package)(\s:\s|:\s)'
      captures: 
        1: minecraft.modstag.color.dark_red
        2: minecraft.modstag.color.tips
      set:
        - meta_scope: minecraft.modstag.color.dark_aqua
        - meta_include_prototype: false
        - match: '\\\\'
          scope: minecraft.modstag.color.dark_aqua
        - include: run
        - match: '[\\/:\?\*<>\|\"]'
          scope: minecraft.modstag.color.error-key
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - include: run
            - include: readme
  readme:
    - match: '(readme|README|Readme)(\s:\s|:\s)'
      captures:
        1: minecraft.modstag.color.dark_red
        2: minecraft.modstag.color.tips
      set:
        - meta_scope: minecraft.modstag.color.dark_aqua
        - meta_include_prototype: false
        - match: '\\\\'
          scope: minecraft.modstag.color.dark_aqua
        - include: run
        - match: '[\\/:\?\*<>\|\"]'
          scope: minecraft.modstag.color.error-key
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          set:
            - include: run
            - include: readme
        - include: run
  run:
    - match: 'run\('
      scope: minecraft.modstag.color.dark_purple
      push:
        - match: '\)\n'
          scope: minecraft.modstag.color.dark_purple
          push:
            - include: config
            
  config:
    - match: '%'
      scope: minecraft.modstag.color.red
      set:
        - match: '%|&'
          scope: minecraft.modstag.color.error-key
        - match: '(cfg)(\.)'
          captures: 
            1: minecraft.modstag.color.aqua
            2: minecraft.modstag.color.tips
          set:
            - match: '(file)(\.)'
              captures: 
                1: minecraft.modstag.color.aqua
                2: minecraft.modstag.color.tips
              set:
                - match: '\\[%&;\\]'
                  scope: minecraft.modstag.color.purple
                - match: '[\[\]:]'
                  scope: minecraft.modstag.color.red
                - match: ';'
                  scope: minecraft.modstag.color.gray
                - match: '&|\\'
                  scope: minecraft.modstag.color.error-key
                - match: '%'
                  scope: minecraft.modstag.color.red
                  set:
                    - match: ':'
                      scope: minecraft.modstag.color.tips
                      set:
                        - match: '\('
                          scope: minecraft.modstag.color.gold
                          set:
                            - include: config.end
                            - include: config-file
                        - match: '\n'
                          scope: minecraft.modstag.color.textwhite
                          push:
                            - clear_scopes: 1
                            - include: config
                    - match: '\n'
                      scope: minecraft.modstag.color.textwhite
                      push:
                        - clear_scopes: 1
                        - include: config
                - match: '\n'
                  scope: minecraft.modstag.color.textwhite
                  push:
                    - clear_scopes: 1
                    - include: config
                - match: '[^\s\n]'
                  scope: minecraft.modstag.color.aqua
            - match: '\n'
              scope: minecraft.modstag.color.textwhite
              push:
                - clear_scopes: 1
                - include: config
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
            - include: config
    - match: '&'
      scope: minecraft.modstag.color.red
      set:
        - match: '\.|%'
          scope: minecraft.modstag.color.error-key
        - match: '&'
          scope: minecraft.modstag.color.red
          push:
            - match: ':'
              scope: minecraft.modstag.color.tips
              set:
                - match: '\('
                  scope: minecraft.modstag.color.gold
                  set:
                    - include: config.end
                    - include: config-file
        - match: '\n'
          scope: minecraft.modstag.color.textwhite
          push:
            - clear_scopes: 1
            - include: config
        - match: '[^\&]'
          scope: minecraft.modstag.color.aqua
    - match: '[^\s\n\:]'
      scope: minecraft.modstag.color.error-key
  config.end:
    - match: '\)'
      scope: minecraft.modstag.color.gold
      set:
        - match: ';'
          scope: minecraft.modstag.color.tips
          set:
            - include: config
  config-file:
    - match: '%'
      scope: minecraft.modstag.color.red
      set:
        - match: '%|&'
          scope: minecraft.modstag.color.error-key
        - match: '(cfg)(\.)'
          captures: 
            1: minecraft.modstag.color.aqua
            2: minecraft.modstag.color.tips
          set:
            - match: '(string)(\.)'
              captures: 
                1: minecraft.modstag.color.aqua
                2: minecraft.modstag.color.tips
              set:
                - match: '\\[%&;\\]'
                  scope: minecraft.modstag.color.purple
                - match: '[\[\]:]'
                  scope: minecraft.modstag.color.red
                - match: ';'
                  scope: minecraft.modstag.color.gray
                - match: '&|\\'
                  scope: minecraft.modstag.color.error-key
                - match: '%'
                  scope: minecraft.modstag.color.red
                  set:
                    - match: '\n'
                      scope: minecraft.modstag.color.error-key
                      set:
                        - clear_scopes: 1
                        - include: config-file
                    - match: ':'
                      scope: minecraft.modstag.color.tips
                      set: 
                        - match: '\n'
                          scope: minecraft.modstag.color.error-key
                          set:
                            - clear_scopes: 1
                            - include: config-file
                        - include: config-file-string
                - match: '\n'
                  scope: minecraft.modstag.color.error-key
                  set:
                    - clear_scopes: 1
                    - include: config-file
                - match: '[^\s\n]'
                  scope: minecraft.modstag.color.aqua
            - match: '(text)(\.)'
              captures: 
                1: minecraft.modstag.color.aqua
                2: minecraft.modstag.color.tips
              set:
                - match: '\\[%&;\\]'
