                          scope: minecraft.modstag.color.error-key
                          set:
                            - clear_scopes: 1
                            - include: config-file
                        - include: config-file-text
                - match: '\n'
                  scope: minecraft.modstag.color.error-key
                  set:
                    - clear_scopes: 1
