              scope: minecraft.modstag.color.tips
              set:
                - include: c-config-file-listExtra-text-list-json-dict-listVar
            - include: c-config-file.end-dict-list-json-dict-list
        - match: '\\[bfnrt\"\\]|\\u[0-9a-fA-F]{4}|\\0o[0-7]{8}|\\0b[01]{32}|%[sv]'
          scope: minecraft.modstag.color.purple
        - match: '\\'
          scope: minecraft.modstag.color.error-key
        - match: '[^\s\n\"]'
          scope: minecraft.modstag.color.yellow
