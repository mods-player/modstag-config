                  scope: minecraft.modstag.color.error-key
                - match: 'auto|\\\"text\\\"'
                  scope: minecraft.modstag.color.white
                - match: '#([0-9a-fA-f]{8}|[0-9a-fA-f]{6}|[0-9a-fA-f]{4}|[0-9a-fA-f]{3})'
                  scope: keyword.operator
                - match: '[^\s\n\#]'
                  scope: punctuation.definition
    - match: '\"var\"'
      scope: minecraft.modstag.color.aqua
      set:
        - meta_include_prototype: false
        - include: c-config-file.notNextError
        - match: ':'
          scope: minecraft.modstag.color.tips
          set:
            - meta_include_prototype: false
            - include: c-config-file.notNextError
            - match: '\['
              scope: minecraft.modstag.color.gold
              set:
                - include: c-config-file-listExtra-text-list-json-dict-listVar
                - include: c-config-file.end-dict-list-json-dict-list
    - match: '\"extra\"'
      scope: minecraft.modstag.color.aqua
      set:
        - meta_include_prototype: false
        - include: c-config-file.notNextError
        - match: ':'
          scope: minecraft.modstag.color.tips
          set: 
            - meta_include_prototype: false
            - include: c-config-file.notNextError
            - match: '\{'
              scope: minecraft.modstag.color.gold
              set:
                - include: c-config-file-listExtra-text-list-json-dict-dictExtra
                - include: c-config-file.end-dict-list-json-dict-dict
  c-config-file-listExtra-text-list-json-dict-listVar:
    - match: '\"'
      scope: minecraft.modstag.color.aqua
      set:
        - meta_include_prototype: false
        - include: c-config-file.notNextError
        - match: '\"'
          scope: minecraft.modstag.color.aqua
          set:
            - meta_include_prototype: false
